{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": [
        "standard"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "plugins": [
        "snakecasejs"
    ],
    "settings":
    {
        "snakecasejs/filter": ["ClassDeclaration", "NewExpression"],
        "snakecasejs/whitelist": ["externalPath", "setNumber"]
    },
    "rules":
    {
        "camelcase": "off",
        "snakecasejs/snakecasejs": "error"
    }
}